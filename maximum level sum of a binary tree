from collections import deque

class Solution:
    def maxLevelSum(self, root):
        q = deque([root])
        level = ans = 1
        max_sum = float('-inf')
        
        while q:
            s = 0
            for _ in range(len(q)):
                n = q.popleft()
                s += n.val
                if n.left: q.append(n.left)
                if n.right: q.append(n.right)
            if s > max_sum:
                max_sum = s
                ans = level
            level += 1
        return ans
